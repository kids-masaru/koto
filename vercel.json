{
    "crons": [
        {
            "path": "/cron?slot=morning",
            "schedule": "0 22 * * *"
        },
        {
            "path": "/cron?slot=noon",
            "schedule": "0 3 * * *"
        },
        {
            "path": "/cron?slot=evening",
            "schedule": "0 9 * * *"
        }
    ],
    "headers": [
        {
            "source": "/api/(.*)",
            "headers": [
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                },
                {
                    "key": "Access-Control-Allow-Methods",
                    "value": "GET,POST,OPTIONS"
                },
                {
                    "key": "Access-Control-Allow-Headers",
                    "value": "Content-Type"
                }
            ]
        }
    ]
}